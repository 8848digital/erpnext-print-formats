 "html": "\n<div class=\"main_div pb0\">\n     {% if letter_head and not no_letterhead %}    \n               {{letter_head}}\n               {% endif %}\n    <div style=\"text-align: right; font-size: 24px;\">REQUEST FOR QUOTATION</div>\n</div>\n<!-- Header End -->\t  \n<div class=\"main_div mt0\">\n   <table class=\"table table-bordered table-condensed mb0 tbspace mt0\" width=\"100%\">\n      <tbody>\n         <tr>\n            <td valign=\"top\" width=\"50%\" rowspan=\"1\">\n              <!-- <p class=\"para_heading bold\">{% if doc.customer_name %}{{ doc.customer_name }}{% else %}{{ doc.customer }}{% endif %}</p>\n               {% if doc.customer_address %}\n               {% set add1, add2, city, pincode, gst_state, country, phone, email = frappe.db.get_value('Address', doc.customer_address, ['address_line1', 'address_line2', 'city', 'pincode', 'gst_state', 'country', 'phone', 'email_id']) %}\n               {% if add1  and add1 is not none %}{{add1}},{% endif %}\n               {% if add2  and add2 is not none %}{{add2}}<br>{% endif %}\n               {% if city  %}{{city}},{% endif %}\n               {% if country  %}{{country}}<br>{% endif %} \n               {% set gstin, gst_state_number = frappe.db.get_value('Address', doc.customer_address, ['gstin', 'gst_state_number']) %}\n               {% if gst_state  %}State Name: {{gst_state}}{% endif %} {% if gst_state_number  %}Code: {{gst_state_number}} {% endif %}\n               <br>\n               {% set pan = frappe.db.get_value(\"Customer\", doc.customer_name, 'pan') %}\n               {% if gstin  %}GSTIN/UIN: {{gstin}}<br>{% endif %}\n               {% if pan  %}PAN: {{pan}}<br>{% endif %}\n               {% if email  %}E-Mail: {{email}}{% endif %} {% if phone  %}Phone: {{phone}}{% endif %}\n               {% endif %}-->\n            </td>\n            <td colspan=\"2\" width=\"25%\" valign=\"top\">\n               <p class=\"mt0\"> RFQ No.</p>\n               <p class=\"bold\">{{doc.name}}</p>\n            </td>\n            <td colspan=\"2\" width=\"25%\" valign=\"top\">\n               <p class=\"mt0\">Dated</p>\n               <p class=\"bold\">{{ frappe.format(doc.transaction_date, {'fieldtype': 'Date'}) }}</p>\n            </td>\n         </tr>\n         \n         <!--Company Address end-->\n         <!-- <tr>\n            <td style=\"padding: 5px;\" valign=\"top\" width=\"50%\">\n            \t<p class=\"mt0\">Consignee</p>\n            \t<p class=\"para_heading bold\">{% if doc.customer_name %}{{ doc.customer_name }}{% else %}{{ doc.customer }}{% endif %}</p>\n            \t\t\n                    {% if doc.shipping_address_name %}\n                          {% set add1, add2, city, pincode, gst_state, country, phone, email = frappe.db.get_value('Address', doc.shipping_address_name, ['address_line1', 'address_line2', 'city', 'pincode', 'gst_state', 'country', 'phone', 'email_id']) %}\n            \n                          {% if add1  and add1 is not none %}{{add1}},{% endif %}\n                          {% if add2  and add2 is not none %}{{add2}}<br>{% endif %}\n                          {% if city  %}{{city}},{% endif %}\n                          {% if country  %}{{country}}<br>{% endif %} \n                          {% set gstin, gst_state_number = frappe.db.get_value('Address', doc.shipping_address_name, ['gstin', 'gst_state_number']) %}\n                          {% if gst_state  %}State Name: {{gst_state}}{% endif %} {% if gst_state_number  %}Code: {{gst_state_number}} {% endif %}\n                          <br>\n                          {% set pan = frappe.db.get_value(\"Customer\", doc.customer_name, 'pan') %}\n                          {% if gstin  %}GSTIN/UIN: {{gstin}}<br>{% endif %}\n                          {% if pan  %}PAN: {{pan}}<br>{% endif %}\n                          {% if email  %}E-Mail: {{email}}{% endif %} {% if phone  %}Phone: {{phone}}{% endif %}\n                          {% endif %}\n            </td>\n            \n            <td width=\"50%\" valign=\"top\" colspan=\"3\">\n            <p class=\"mt0\">Buyer (if other than consignee)</p>\n            \t<p class=\"para_heading bold\">{% if doc.customer_name %}{{ doc.customer_name }}{% else %}{{ doc.customer }}{% endif %}</p>\n                    {% if doc.customer_address %}\n                          {% set add1, add2, city, pincode, gst_state, country, phone, email = frappe.db.get_value('Address', doc.customer_address, ['address_line1', 'address_line2', 'city', 'pincode', 'gst_state', 'country', 'phone', 'email_id']) %}\n            \n                          {% if add1  and add1 is not none %}{{add1}},{% endif %}\n                          {% if add2  and add2 is not none %}{{add2}}<br>{% endif %}\n                          {% if city  %}{{city}},{% endif %}\n                          {% if country  %}{{country}}<br>{% endif %} \n                          {% set gstin, gst_state_number = frappe.db.get_value('Address', doc.customer_address, ['gstin', 'gst_state_number']) %}\n                          {% if gst_state  %}State Name: {{gst_state}}{% endif %} {% if gst_state_number  %}Code: {{gst_state_number}} {% endif %}\n                          <br>\n                          {% set pan = frappe.db.get_value(\"Customer\", doc.customer_name, 'pan') %}\n                          {% if gstin  %}GSTIN/UIN: {{gstin}}<br>{% endif %}\n                          {% if pan  %}PAN: {{pan}}<br>{% endif %}\n                          {% if email  %}E-Mail: {{email}}{% endif %} {% if phone  %}Phone: {{phone}}{% endif %}\n                          {% endif %}\n            </td>\n            \n            </tr>-->\n         <!--Consignee and Buyer address end-->\n      </tbody>\n   </table>\n   <!--One Step Done-->\n   <br>\n   <table class=\"table table-bordered table-condensed mb0 tbspace mt0 \" width=\"100%\" style=\"font-size: 11px;\">\n      <thead>\n         <tr>\n            <th class=\"text-center\" width=\"7%\">Sr No</th>\n            <th class=\"text-center\" width=\"45%\">Description</th>\n            <th class=\"text-center\" width=\"10%\">Reuired Date</th>\n            <th class=\"text-center\" width=\"10%\">Quantity</th>\n          <!--  <th class=\"text-center\" width=\"12%\">Rate</th>\n            <th class=\"text-center\" width=\"12%\">Amount</th>-->\n         </tr>\n      </thead>\n      <tbody>\n         {% for item in doc.items %}\n         <tr>\n            <td class=\"text-center\"> {{item.idx}} </td>\n            <td class=\"text-left\">\n               <p class=\"mt0\"> <span class=\"bold\">{{ item.item_name }}:</span> {{ item.item_code }} </p>\n               <p class=\"mt0\">{{ item.description |striptags }}</p>\n            </td>\n            <td class=\"text-left\"> {{item.schedule_date }} </td>\n            <td class=\"text-right\"> {{item.uom}} {{'%0.2f' %  item.qty }} </td>\n   <!--         <td class=\"text-right\">{{ item.get_formatted('rate') }}</td>\n            <td class=\"text-right\">{{ item.get_formatted('amount') }}</td>-->\n         </tr>\n         {% endfor %}\n      </tbody>\n   </table>\n   {% if doc.terms and doc.terms is not none %}\n   <div style=\"border: 1px solid #ddd;padding: 5px;page-break-inside:avoid;\">\n      <p class=\"bold\">Terms and Conditions:</p>\n      <p class=\"mb0 mb0\" style=\"font-size:11px;page-break-inside:avoid;\"> {{doc.terms}} </p>\n   </div>\n   <div style=\"clear:both\"></div>\n   {% endif %}\n   <table class=\"table table-bordered tbspace  mt0 mb0 \" width=\"100%\">\n      <tbody>\n         <tr>\n            <td class=\"text-right\" colspan=\"2\">\n               <p class=\"para_heading bold mt0\" >For <span class=\"text-uppercase\"> {{doc.company}}</span></p>\n               <p class=\"sign_div\"></p>\n               <p class=\"mb0\">Authorised Signatory</p>\n            </td>\n         </tr>\n      </tbody>\n   </table>\n</div>",
